{
  "name": "contest-frontend-quasar",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "Vladimir Sitchikhin <vsitchikhin@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . && stylelint \"**/*.scss\" --config",
    "format": "npm run format:eslint && npm run format:stylelint",
    "format:eslint": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore .",
    "format:stylelint": "stylelint \"**/*.scss\" --fix --config",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "api:generate": "ts-node --skip-project ./api-generate-cli.ts",
    "api:generate:dev": "bash generate-api.sh",
    "build": "quasar build"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "pinia": "^2.0.11",
    "@quasar/extras": "^1.16.4",
    "@vueuse/core": "^9.13.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.10.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "@types/node": "^12.20.21",
    "@quasar/app-vite": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
